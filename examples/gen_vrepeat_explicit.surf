let aux = rec(self : (q : Nat) -> {p : Nat} -> Vec p -> Vec (p * q)) ->
  fun(m : Nat) -> fun{n : Nat} -> fun(e : Vec n) ->
    if m <= 0 then
      [| |]
    else
      vconcat {n} {n * (m - 1)} e (self (m - 1) {n} e)
in
let vrepeat (m : Nat) {n : Nat} (v : Vec n) = aux m {n} v in
vrepeat 5 {3} [| 4; 6; 9 |]
